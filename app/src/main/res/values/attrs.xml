<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingComponent">
        <attr name="propertyName" format="string" />
        <attr name="infoDescription" format="reference" />
        <attr name="dbColumnName" format="string" />
        <attr name="infoImage" format="reference" />
        <attr name="valueMin" format="float" />
        <attr name="valueMax" format="float" />
        <attr name="stepSize" format="float" />
        <attr name="hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ImageCategorySelector">
        <attr name="meterType" format="enum">
            <enum name="GAS" value="0" />
            <enum name="ELECTRIC" value="1" />
        </attr>
        <attr name="comboBoxHeight" format="integer" />
    </declare-styleable>
</resources>