<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:angle="0"
            android:innerRadiusRatio="2.4"
            android:shape="ring"
            android:thicknessRatio="12.0"
            android:type="sweep"
            android:useLevel="false">
            <solid android:color="@color/green_400" />
        </shape>
    </item>
    <item
        android:width="72dp"
        android:height="72dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:width="21dp"
        android:height="5dp"
        android:gravity="center"
        android:right="18dp"
        android:top="15dp">
        <rotate android:fromDegrees="40">
            <shape android:shape="rectangle">
                <solid android:color="@color/green_400" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="32dp"
        android:height="5dp"
        android:gravity="center"
        android:left="12dp"
        android:top="4dp">
        <rotate android:fromDegrees="-50">
            <shape android:shape="rectangle">
                <solid android:color="@color/green_400" />
            </shape>
        </rotate>
    </item>
</layer-list><!--<layer-list xmlns:android="http://schemas.android.com/apk/res/android">-->
