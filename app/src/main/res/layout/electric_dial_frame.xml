<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/electricDialFrame"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/electric_dial_frame_desc"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dial_frame_electric" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineBottom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.79" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.16" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineWholeDialStart2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineWholeDialEnd2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.78" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineFractionalDialStart2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.84" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineFractionalDialEnd2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.955" />

    <NumberPicker
        android:id="@+id/numberPicker10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:chainUseRtl="false"
        app:layout_constraintBottom_toTopOf="@+id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/numberPicker12"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/guidelineWholeDialStart2"
        app:layout_constraintTop_toTopOf="@+id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:layout_constraintBottom_toTopOf="@+id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/numberPicker13"
        app:layout_constraintTop_toTopOf="@+id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker13"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:chainUseRtl="false"
        app:layout_constraintBottom_toBottomOf="@id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/numberPicker14"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintTop_toTopOf="@id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker14"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:layout_constraintBottom_toBottomOf="@id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/numberPicker15"
        app:layout_constraintTop_toTopOf="@id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker15"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:layout_constraintBottom_toBottomOf="@id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/numberPicker16"
        app:layout_constraintTop_toTopOf="@id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:layout_constraintBottom_toBottomOf="@id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/guidelineWholeDialEnd2"
        app:layout_constraintTop_toTopOf="@id/guidelineTop2"
        app:layout_constraintWidth_percent="0.12" />

    <NumberPicker
        android:id="@+id/numberPicker17"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:descendantFocusability="blocksDescendants"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:showDividers="none"
        android:theme="@style/dialTheme"
        app:layout_constraintBottom_toBottomOf="@id/guidelineBottom2"
        app:layout_constraintEnd_toStartOf="@id/guidelineFractionalDialEnd2"
        app:layout_constraintStart_toStartOf="@id/guidelineFractionalDialStart2"
        app:layout_constraintTop_toTopOf="@id/guidelineTop2"
        app:layout_constraintWidth_percent="0.11" />

</androidx.constraintlayout.widget.ConstraintLayout>